/* Write a program to find the biggest of 3 Numbers 

import java.util.*;
public class BiggestOf3 {
	public static void main(String args[]) {
		System.out.println("Hariom Dhakar");
		System.out.println("Enter value of A");
		Scanner sc = new Scanner(System.in);
		int a=sc.nextInt();
		System.out.println("Enter value of B");
		sc = new Scanner(System.in);
		int b=sc.nextInt();
		System.out.println("Enter value of C");
		sc = new Scanner(System.in);
		int c=sc.nextInt();
		if(a>b && a>c) {
			System.out.println("A is the biggest number "+ a);
		}
		else if(b>a && b>c) {
			System.out.println("B is the biggest number "+ b);
		}
		else {
			System.out.println("C is the biggest number "+ c);
		}
	}
}
