// Write a java program to print sum of n terms in the series 
//1/1! + 1/2! + 1/3! + 1/4! +......

import java.util.*;
public class SumSeries {
public static void main(String [] args) {
	System.out.println("Hariom Dhakar");
	Scanner sc = new Scanner(System.in);
	int n=sc.nextInt();
	double ans = 0.0;
	
	for(int i=1;i<=n;i++) {
		int fact=1;
		for(int j=1;j<=i;j++) {
			fact*=j;
		}
		ans+=1.0/fact;
	}
	System.out.println(ans);
}

}
